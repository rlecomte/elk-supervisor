{
  "template":"prod-*",
  "settings":{

  },
  "mappings":{
    "_default_" : {
    },
    "METER":{
      "_all":{
        "enabled":false
      },
      "_source":{
        "compress":true
      },
      "dynamic_templates":[
        {
          "metric_template":{
            "match":"*-int",
            "mapping":{
              "type":"long",
              "index":"not_analyzed"
            },
            "match_mapping_type":"long"
          }
        },
        {
          "string_template":{
            "match":"*",
            "mapping":{
              "type":"string"
            },
            "match_mapping_type":"string"
          }
        }
      ],
      "properties":{
        "@timestamp":{
          "type":"date",
          "format":"dateOptionalTime"
        },
        "@version":{
          "type":"string"
        }
      }
    }
  }
}